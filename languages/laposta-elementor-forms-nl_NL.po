#: includes/class-laposta-action.php:39 includes/class-laposta-action.php:55
msgid "Laposta"
msgstr "Laposta"

#: includes/class-laposta-action.php:65
msgid "API Key"
msgstr "API-sleutel"

#: includes/class-laposta-action.php:69
msgid "Enter the Laposta API key found in your Laposta account settings."
msgstr "Voer de Laposta API-sleutel in die je vindt in de Laposta-accountinstellingen."

#: includes/class-laposta-action.php:74
msgid "List"
msgstr "Lijst"

#: includes/class-laposta-action.php:78
msgid "Choose the Laposta list that should receive the submitted form entries."
msgstr "Kies de Laposta-lijst waarin de verzonden formulierinzendingen terecht moeten komen."

#: includes/class-laposta-action.php:86
msgid "Update existing subscriber (upsert)"
msgstr "Bestaande abonnee bijwerken (upsert)"

#: includes/class-laposta-action.php:88
msgid "Yes"
msgstr "Ja"

#: includes/class-laposta-action.php:89
msgid "No"
msgstr "Nee"

#: includes/class-laposta-action.php:92
msgid "If enabled, an existing subscriber with the same email will be updated instead of causing an error."
msgstr "Indien ingeschakeld wordt een bestaande abonnee met hetzelfde e-mailadres bijgewerkt in plaats van een foutmelding te geven."

#: includes/class-laposta-action.php:98
msgid "API fields mapping"
msgstr "API-velden koppelen"

#: includes/class-laposta-action.php:107
msgid "Map the form fields to the Laposta fields."
msgstr "Koppel de formuliervelden aan de Laposta-velden."

#: includes/class-laposta-action.php:219
msgid "Unable to communicate with Laposta at the moment."
msgstr "Kan momenteel geen verbinding maken met Laposta."

#: includes/class-laposta-action.php:228 includes/class-laposta-action.php:231 includes/class-laposta-action.php:234
msgid "This email address is already subscribed."
msgstr "Dit e-mailadres is al geabonneerd."

#: includes/class-laposta-action.php:237
msgid "An error occurred while subscribing."
msgstr "Er is een fout opgetreden tijdens het inschrijven."

#: laposta-elementor-forms.php:223
msgid "Form option"
msgstr "Formulieroptie"

#: laposta-elementor-forms.php:224
msgid "Laposta option"
msgstr "Laposta-optie"

#: laposta-elementor-forms.php:225
msgid "Select…"
msgstr "Selecteer…"

#: laposta-elementor-forms.php:226
msgid "Select a form field with options to create a mapping."
msgstr "Selecteer een formulier veld met opties om een koppeling te maken."

#: laposta-elementor-forms.php:227
msgid "This Laposta field has no options."
msgstr "Dit Laposta-veld heeft geen opties."

#: laposta-elementor-forms.php:228
msgid "No additional mapping needed for this field."
msgstr "Geen extra koppeling nodig voor dit veld."

#: laposta-elementor-forms.php:229
msgid "Append new selections to existing subscriber (upsert)"
msgstr "Nieuwe selecties toevoegen aan bestaande abonnee (upsert)"

#: laposta-elementor-forms.php:230
msgid "Leave unchecked to overwrite existing selections when the subscriber already exists."
msgstr "Laat dit uit om bestaande selecties te overschrijven wanneer de abonnee al bestaat."

#: laposta-elementor-forms.php:231
msgid "Allow creating new Laposta options"
msgstr "Nieuwe Laposta-opties toestaan"

#: laposta-elementor-forms.php:232
msgid "Add hidden field values that are not present yet as selectable options in Laposta."
msgstr "Voeg verborgen veldwaarden die nog niet bestaan toe als selecteerbare opties in Laposta."

#: laposta-elementor-forms.php:233
msgid "Hidden fields can be mapped to Laposta multi-select fields. Provide comma separated values in the default value."
msgstr "Verborgen velden kunnen aan Laposta multi-select velden gekoppeld worden. Gebruik komma's in de standaardwaarde om meerdere opties op te geven."

#: laposta-elementor-forms.php:234
msgid "Hidden field"
msgstr "Verborgen veld"

#: laposta-elementor-forms.php:235
msgid "Loading…"
msgstr "Bezig met laden…"

#: laposta-elementor-forms.php:236
msgid "Select a list"
msgstr "Selecteer een lijst"

#: laposta-elementor-forms.php:237
msgid "Failed to fetch Laposta lists. Please check your API key."
msgstr "Het ophalen van Laposta-lijsten is mislukt. Controleer je API-sleutel."

#: laposta-elementor-forms.php:238
msgid "Failed to fetch Laposta fields."
msgstr "Het ophalen van Laposta-velden is mislukt."

#: laposta-elementor-forms.php:239 laposta-elementor-forms.php:388
msgid "No Laposta lists found."
msgstr "Geen Laposta-lijsten gevonden."

#: laposta-elementor-forms.php:240 laposta-elementor-forms.php:454
msgid "Unable to fetch Laposta fields for the selected list."
msgstr "Kan Laposta-velden voor de geselecteerde lijst niet ophalen."

#: laposta-elementor-forms.php:242
#, php-format
msgid "<strong>Laposta Elementor Forms</strong> requires <a href=\"%s\" target=\"_blank\"><strong>Elementor PRO</strong></a> to be installed and activated."
msgstr "<strong>Laposta Elementor Forms</strong> vereist dat <a href=\"%s\" target=\"_blank\"><strong>Elementor PRO</strong></a> is geïnstalleerd en geactiveerd."

#: laposta-elementor-forms.php:266
#, php-format
msgid "<strong>Laposta Elementor Forms</strong> requires <strong>Elementor</strong> version %s or greater."
msgstr "<strong>Laposta Elementor Forms</strong> vereist <strong>Elementor</strong> versie %s of hoger."

#: laposta-elementor-forms.php:396
msgid "Invalid Laposta API key."
msgstr "Ongeldige Laposta API-sleutel."

#: laposta-elementor-forms.php:407
msgid "Connectivity issue: unable to retrieve Laposta lists."
msgstr "Verbindingsprobleem: Laposta-lijsten kunnen niet worden opgehaald."

#: laposta-elementor-forms.php:421
msgid "Missing Laposta request parameters."
msgstr "Ontbrekende Laposta-parameters."

#: laposta-elementor-forms.php:430
msgid "Invalid Laposta request parameters."
msgstr "Ongeldige Laposta-parameters."

#: laposta-elementor-forms.php:442
msgid "Connectivity issue: unable to reach Laposta right now."
msgstr "Verbindingsprobleem: Laposta is momenteel niet bereikbaar."
